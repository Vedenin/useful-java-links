{
  "/": {
    "device": "/dev/sda1",
    "options": [ "ssd" ]
  },
  "swap": {
    "device": "/dev/sda2",
    "fstype": "swap"
  },
  "/tmp": {
    "device": "tmpfs",
    "fstype": "tmpfs",
    "options": "size=64M"
  },
  "/var/lib/mysql": {
    "device": "/dev/data/mysql",
    "fstype": "btrfs"
  }
}